{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["."],
    "exclude": [
      "node_modules",
      "docs",
      "test",
      "bin",
      "obj",
      "Services/GdalServiceLib.cs"
    ],
    //"includePattern": "NewsModel.cs$"
    "includePattern": "\\.cs$"
  },
  "plugins": ["plugins/markdown", "jsdoc-parser-cs.cjs"],
  "opts": {
    "encoding": "utf8",
    "destination": "docs-backend/",
    "readme": "jsdoc-backend.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
